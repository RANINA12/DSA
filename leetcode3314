class Solution {
    public int[] minBitwiseArray(List<Integer> nums) {
        int ans[] = new int[nums.size()];

        for(int i =0 ; i<nums.size() ;i++){
            
            ans[i]=solve(nums.get(i));
        }

        return ans;
    }

    int solve(int val){
        for(int i =0; i<val ;i++){
            if((i | i+1)==val) return i;
        }

        return -1;
    }
}
