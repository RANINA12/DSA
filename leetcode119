class Solution {
    public List<Integer> getRow(int rowIndex) {
        List<Integer>  ans = new ArrayList<>();
         ans.add(1);
         for(int i =1 ; i<rowIndex+1;i++){
           int prev =0;
            List<Integer> temp =  new ArrayList<>();
           for(int j =0 ; j<ans.size();j++){
            temp.add(prev+ ans.get(j));
            prev = ans.get(j);
           }
           temp.add(1);
           ans = new ArrayList<>(temp); 
        }
        return ans;
    }
}
