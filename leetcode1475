class Solution {
    public int[] finalPrices(int[] prices) {
        for(int i =0 ; i<prices.length ;i++){
            int j = i+1;
            int a = prices[i];
            while(j<prices.length && a<prices[j]){
                j++;
            }
            j = j>=prices.length ? 0 : prices[j];
            prices[i]=prices[i]-j;
        }
        return prices;
    }
}
