class Solution {
    public int countOperations(int a, int b) {
        int op = 0;
        while (a > 0 && b > 0) {
            if (a >= b) {
                op = op + a / b;
                a = a % b;
            } else {
                op = op + b / a;
                b = b % a;
            }
        }
        return op;
    }
}
