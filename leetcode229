class Solution {
    public List<Integer> majorityElement(int[] nums) {
        int n = nums.length ; 
        int k = n/3;
        List<Integer> li = new ArrayList<>();
        HashMap<Integer , Integer> map = new HashMap<>();
        for(int i =0 ; i<n ; i++){
            map.put(nums[i], map.getOrDefault(nums[i], 0)+1);
        }

        for(int a : map.keySet()){
            if(map.get(a) > k) li.add(a);
        }
        return li;
    }
}
