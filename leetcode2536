class Solution {
    private void add(int[][] arr  , int[] q ){

        for(int i = q[0] ; i<=q[2] ;i++){
            for(int j =q[1]; j <= q[3] ; j++){
                arr[i][j]++;
            }
        }

    }
    public int[][] rangeAddQueries(int n, int[][] queries) {

        int[][] arr = new int[n][n];
         for(int[] q : queries){
            add(arr , q);
         }

         return arr;
    }
}
