class Solution {
    public int distributeCandies(int[] c) {
        int n = c.length;
        HashSet<Integer> set = new HashSet<>();
        for(int  i =0 ; i<n ; i++){
           set.add(c[i]);
        }
        int caneat = n/2;
        if(caneat >= set.size()) return set.size();
        else return caneat;
    }
}
