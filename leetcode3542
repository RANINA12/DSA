class Solution {
    public int minOperations(int[] nums) {
     int op =0;
     Stack<Integer> st = new Stack<>();

     for(int i =0 ; i<nums.length ;i++){
        if(nums[i]==0){
            st.clear();
            continue;
        }
         while(!st.isEmpty() && st.peek()>nums[i]){
            st.pop();
         }

         if(st.isEmpty() || st.peek()!= nums[i]){
            op++;
            st.push(nums[i])   ;      }
     }

     return op;
    }
}
