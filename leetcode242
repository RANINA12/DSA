class Solution {
    public boolean isAnagram(String s, String t) {
        if(s.length() >t.length()) return false;
        char[] c = new char[26];

        for(int i=0 ; i<s.length();i++){
            char ch = s.charAt(i);
            c[ch-'a']++;
        }
        for(int i=0 ; i<t.length();i++){
            char ch = t.charAt(i);
            if(c[ch-'a']==0) return false;
            c[ch-'a']--;
        }

        return true;
    }
}
